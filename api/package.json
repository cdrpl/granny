{
  "name": "api",
  "version": "0.1.0",
  "main": "./dist/src/main.js",
  "type": "module",
  "scripts": {
    "start": "node -r dotenv/config ./dist/src/main.js",
    "tsc-start": "tsc && npm start",
    "dev-start": "nodemon -r dotenv/config ./dist/src/main.js",
    "tsc": "tsc",
    "test": "cross-env NODE_ENV=test DB_USER=test mocha --require dotenv/config ./dist/test --exit --recursive",
    "seed": "knex seed:run",
    "migrate:up": "knex migrate:latest",
    "migrate:down": "knex migrate:rollback"
  },
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/express": "^4.17.11",
    "@types/mocha": "^8.2.1",
    "@types/morgan": "^1.9.2",
    "@types/redis": "^2.8.28",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "chai": "^4.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.20.0",
    "mocha": "^8.3.0",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.3",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.10.0",
    "knex": "^0.95.0-next3",
    "morgan": "^1.10.0",
    "pg": "^8.5.1",
    "redis": "^3.0.2"
  }
}
